{
  "name": "nextjs-seed-app",
  "version": "1.0.0",
  "description": "PatternFly Next.js Starter Generator - Framework-agnostic component generation system",
  "main": "index.js",
  "scripts": {
    "test": "jest --config config/jest.config.js",
    "test:watch": "jest --watch --config config/jest.config.js",
    "test:integration": "jest --config config/jest.config.js tests/integration",
    "test:e2e": "jest --config config/jest.config.js tests/e2e",
    "build": "tsc --project config/tsconfig.json",
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "cli": "ts-node modules/cli/patternfly-cli.ts",
    "test:cli": "jest --config config/jest.config.js --testPathPattern=cli",
    "test:schema": "jest --config config/jest.config.js --testPathPattern=schema-extractor",
    "test:templates": "jest --config config/jest.config.js --testPathPattern=template-engine",
    "test:isolation": "jest --config config/jest.config.js --testPathPattern=component-isolation"
  },
  "keywords": [
    "patternfly",
    "nextjs",
    "react",
    "starter",
    "schema",
    "extraction"
  ],
  "author": "Michael Nolting",
  "license": "MIT",
  "dependencies": {
    "@patternfly/react-core": "^6.4.0",
    "@patternfly/react-styles": "^6.4.0",
    "@types/node": "^20.0.0",
    "next": "^16.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@types/jest": "^29.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0"
  }
}
